{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true,
        "browser": true
    },
    "settings": {
      "react": {
        "pragma": "React",
        "version": "16.12.0"
      }
    },
    "extends": "airbnb",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "plugins": [
      "react",
      "jsx-a11y",
      "import"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        // "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
      "no-console": "warn",
      "react/jsx-filename-extension": [1, {"extensions": [".js", ".jsx"] }]
    }
  }